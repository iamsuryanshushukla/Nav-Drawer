<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.demoproject.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.demoproject.ui.contacts.ContactsFragment"
        android:label="@string/title_contact"
        tools:layout="@layout/fragment_contacts" />

    <fragment
        android:id="@+id/navigation_files"
        android:name="com.example.demoproject.ui.files.FilesFragment"
        android:label="@string/title_files"
        tools:layout="@layout/fragment_files" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.demoproject.Fragments.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting"/>
    <fragment
        android:id="@+id/musicFragment"
        android:name="com.example.demoproject.Fragments.MusicFragment"
        android:label="MusicFragment" />
</navigation>